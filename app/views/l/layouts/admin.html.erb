<!DOCTYPE html>
<html>
  <head>
    <title><%= [t('admin.title'), '::', yield_title] * ' ' %></title>
    <meta name="description" content="<%= yield_description %>" />
    <meta name="keywords" content="<%= yield_keywords %>" />
    <%= stylesheet_link_tag 'admins' %>
    <%= tiny_mce_uploads_stylesheets_if_needed %>
    <!--[if lt IE 9]><%= javascript_include_tag 'ie8polyfill' %><![endif]-->
    <%= javascript_include_tag 'admins' %>
    <%= tiny_mce_uploads_javascripts_if_needed %>
    <%= yield :head %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id="content_tabs">
      <%= render :partial => "l/admins/partials/header" %>
      <%= render :partial => "l/admins/partials/content" %>
    </div>
    <%= render :partial => "l/admins/partials/footer" %>
  </body>
</html>
