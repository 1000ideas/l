<% title t('.title') %>
<% breadcrumbs_admin link_to(t('.title'), admin_pages_path) %>

<p id="notice"><%= notice %></p>

<ul class="items-list two-columns sortable" data-url="<%= after_admin_page_path(id: ':id', target_id: ':target_id') %>">
  <li class="header">
    <div class="group">
      <div><%= L::Page.human_attribute_name :title %></div>
      <div><%= L::Page.human_attribute_name :url %></div>
    </div>
  </li>
  <% @pages.each do |page| %>
    <%= render 'page_list_item', page: page %>
  <% end %>
</ul>
