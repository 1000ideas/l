<% unless @gallery.gallery_photos(true).blank? %>
<h2><%= t('.photos_added') %></h2>
<ul class="photos-list">
    <% @gallery.gallery_photos(true).each do |photo| %>
      <li>
      <%= collection_lightbox_tag photo.photo, 'gallery', original_style: :big, target: '_blank', class: 'photo-link' %>
      <%= link_to t('helpers.actions.destroy'), admin_gallery_photo_path(gallery_id: photo.gallery_id, id: photo.id), class: [:button, :white, :expand, :tiny, :'remove-photo'], data: { confirm: t('helpers.actions.confirm'), method: :delete, remote: true } %>
      </li>
    <% end %>
</ul>
<% end %>
